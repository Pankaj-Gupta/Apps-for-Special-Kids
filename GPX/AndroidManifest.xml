<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gpx"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        
        <activity
            android:name="com.example.gpx.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.gpx.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.gpx.Menui"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.gpx.MENUI" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.gpx.Sir"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.gpx.SIR" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.gpx.hello"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.gpx.HELLO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        
        <activity
            android:name="com.example.gpx.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.gpx.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.gpx.Start"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                
            </intent-filter>
        </activity>
        
        
        
    </application>

</manifest>